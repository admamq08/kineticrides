<!DOCTYPE html>
<html>
  <head>
    <title>Unlock Bike</title>
  </head>
  <body>
    <h1>Unlock the Bike</h1>
    <button id="unlock-btn">Unlock</button>

    <script>
      document.getElementById('unlock-btn').addEventListener('click', () => {
        fetch('http://<192.168.0.175>/unlock', {
          method: 'GET'
        })
        .then(response => {
          if (response.ok) {
            alert('Bike Unlocked!');
          }
        })
        .catch(err => console.error('Error:', err));
      });
    </script>
  </body>
</html>
